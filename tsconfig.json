{
	"version": "1.8.10",
	"compilerOptions": {
		"declaration": false,
		"module": "umd",
		"noImplicitAny": true,
		"outDir": "_build/",
		"removeComments": false,
		"sourceMap": true,
		"target": "es6",
		"baseUrl": "node_modules",
		"paths": {
			"util/*": [
				"src/util/*"
			],
			"commands/*": [
				"src/commands/*"
			]
		}
	},
	"include": [
		"./typings/index.d.ts",
		"./src/commands/*.ts",
		"./src/util/*.ts",
		"./src/lib/*.ts",
		"./tests/**/*.ts"
	]
}
